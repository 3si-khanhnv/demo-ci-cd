{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"imo-client": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "imo",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/imo-client",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"assets": [
							"src/favicon.ico",
							{
								"glob": "**/*",
								"input": "src/assets",
								"output": "assets"
							}
						],
						"styles": [
							"src/styles.scss",
							"node_modules/ngx-toastr/toastr.css"
						],
						"scripts": [
							"node_modules/apexcharts/dist/apexcharts.min.js"
						],
						"vendorChunk": true,
						"extractLicenses": true,
						"buildOptimizer": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true,
						"allowedCommonJsDependencies": [
							"dayjs",
							"clone-deep",
							"pdfmake/build/pdfmake",
							"rxjs/internal/Subject",
							"apexcharts",
							"util",
							"moment",
							"file-saver",
							"html2canvas",
							"pdfmake/build/vfs_fonts",
							"rfdc"
						],
						"extraWebpackConfig": "webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "none",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [],
							"extraWebpackConfig": "webpack.prod.config.js"
						},
						"e2e": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.e2e.ts"
								}
							],
							"outputHashing": "none",
							"sourceMap": false,
							"namedChunks": false,
							"vendorChunk": false,
							"budgets": []
						},
						"dev": {
							"optimization": false,
							"outputHashing": "none",
							"sourceMap": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": false
						}
					},
					"defaultConfiguration": ""
				},
				"test": {
					"builder": "@angular-builders/jest:run",
					"options": {
						"configPath": "jest.config.js"
					}
				},
				"storybook": {
					"builder": "@storybook/angular:start-storybook",
					"options": {
						"configDir": ".storybook",
						"styles": [
							"projects/glory-imo-dev/shared-components/src/lib/styles.scss",
							"node_modules/ngx-toastr/toastr.css"
						],
						"browserTarget": "imo-client:build",
						"compodoc": false,
						"port": 6006
					}
				},
				"build-storybook": {
					"builder": "@storybook/angular:build-storybook",
					"options": {
						"configDir": ".storybook",
						"browserTarget": "imo-client:build",
						"compodoc": false,
						"outputDir": "storybook-static"
					}
				}
			}
		},
		"@glory-imo-dev/shared-components": {
			"projectType": "library",
			"root": "projects/glory-imo-dev/shared-components",
			"sourceRoot": "projects/glory-imo-dev/shared-components/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/glory-imo-dev/shared-components/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/glory-imo-dev/shared-components/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/glory-imo-dev/shared-components/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"tsConfig": "projects/glory-imo-dev/shared-components/tsconfig.spec.json",
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						]
					}
				}
			}
		}
	},
	"cli": {
		"analytics": false
	}
}